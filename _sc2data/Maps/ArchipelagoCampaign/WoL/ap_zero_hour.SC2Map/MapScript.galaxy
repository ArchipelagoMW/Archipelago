    lv_lastGroup = UnitGroupEmpty();

    // Implementation
    libNtve_gf_PlayAnimation(libNtve_gf_MainActorofUnit(gv_nydusWormP03), c_animNameDefault, "Spell", 0, c_animTimeDefault);
