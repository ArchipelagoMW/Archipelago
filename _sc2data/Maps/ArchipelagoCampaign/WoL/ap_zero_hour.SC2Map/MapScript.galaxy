    auto1A72C8F9_g = PlayerGroupAll();
    auto1A72C8F9_var = -1;
    while (true) {
        auto1A72C8F9_var = PlayerGroupNextPlayer(auto1A72C8F9_g, auto1A72C8F9_var);
