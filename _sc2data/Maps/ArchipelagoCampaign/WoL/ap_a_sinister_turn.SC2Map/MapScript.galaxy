    libNtve_gf_SetAlliance(gv_p1_USER, gv_p3_PROTOSS_ENEMY, libNtve_ge_AllianceSetting_Enemy);
    libNtve_gf_SetAlliance(gv_p1_USER, gv_p4_PROTOSS_ENEMY, libNtve_ge_AllianceSetting_Enemy);
