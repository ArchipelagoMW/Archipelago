    // Automatic Variable Declarations
    playergroup autoD3ACB033_g;
    int autoD3ACB033_var;

    // Variable Initialization
    lv_difficulty = 2;

    // Actions
    if (!runActions) {
        return true;
    }

    lv_difficulty = libC6542F50_gf_APSetDifficulty(StringWord(EventChatMessage(false), 2), UnitGroupFromId(1), UnitGroupFromId(2), UnitGroupFromId(3), UnitGroupFromId(4));
    autoD3ACB033_g = PlayerGroupAll();
    autoD3ACB033_var = -1;
    while (true) {
        autoD3ACB033_var = PlayerGroupNextPlayer(autoD3ACB033_g, autoD3ACB033_var);
        if (autoD3ACB033_var < 0) { break; }
        PlayerSetDifficulty(autoD3ACB033_var, lv_difficulty);
    }
    TriggerExecute(libC6542F50_gt_APPreInitMap, true, true);
    TriggerExecute(gt_Init02Players, true, false);
    TriggerExecute(gt_Initialization, true, true);
    lib1_gf_initchatbox(c_anchorTopRight, 500, 350, 0, 40);
    lib1_gf_showhidechatbox(1, true);
    TechTreeRestrictionsEnable(1, true);
    TriggerExecute(libC6542F50_gt_APClearTech, true, false);
    libCamp_gf_EnableCampaignTechUnit(true, libCamp_ge_StoryTechGroup_StoryTechGroupBaseSCV, 1);
    libC6542F50_gf_APUnlockUnits(StringWord(EventChatMessage(false), 3), StringWord(EventChatMessage(false), 14));
