//--------------------------------------------------------------------------------------------------
// Trigger: Victory Purifier Destroyed
//--------------------------------------------------------------------------------------------------
bool gt_VictoryPurifierDestroyed_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Conditions
    if (testConds) {
        if (!((gv_gameOver == false))) {
            return false;
        }

        if (!((UnitIsAlive(gv_purifier) == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    TriggerEnable(TriggerGetCurrent(), false);
    lib5BD4895D_gf_AP_Core_sendLocationLocal(lib5BD4895D_gv_aP_Core_locationVictory);
    if ((gv_firstFleetObjectiveFailed == false) && (gv_firstTerrorFleetStopped == false)) {
        lib5BD4895D_gf_AP_Core_sendLocationLocal(4);
    }

    if ((gv_secondFleetObjectiveFailed == false) && (gv_secondTerrorFleetStopped == false)) {
        lib5BD4895D_gf_AP_Core_sendLocationLocal(5);
    }

    if ((gv_thirdFleetObjectiveFailed == false) && (gv_thirdTerrorFleetStopped == false)) {
        lib5BD4895D_gf_AP_Core_sendLocationLocal(6);
    }

    Wait(1.0, c_timeGame);
    gv_gameOver = true;
